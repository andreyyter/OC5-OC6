def maxprem(local):
    maxi = 0
    for i in range(1,kol):
        if (local[i] >= local[i-1]):
            maxi = i
    for i in range(maxi + 1, kolvo):
        local[i] = 0
        
        kol = int(input('Введите  элементы'))
